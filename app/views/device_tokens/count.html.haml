- content_for :title do
  Dubsar Device Tokens
- content_for :description do
  %link{:rel => 'canonical', :href => 'https://dubsar.info/device_tokens/count'}

= render partial: 'home_button'
%h2 Dubsar APNS Device Token Counts
%h3 #{DateTime.now.strftime("%Y-%b-%d %H:%M:%S %Z")}

%table#device_counts
  %thead
    %tr
      %th prod
      %th dev
  %tbody
    %tr
      %td #{@prod_count.to_s delimiter: ','}
      %td #{@dev_count.to_s delimiter: ','}
